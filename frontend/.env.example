# React App Environment Variables
# NOTE: All React env vars must start with REACT_APP_ or VITE_ depending on your build tool

# API Configuration
REACT_APP_API_BASE_URL=https://api-staging.missionctrl.app/api

# Firebase Configuration
REACT_APP_FIREBASE_PROJECT_ID=groundctrl-staging
REACT_APP_FIREBASE_API_KEY=your-web-api-key-from-firebase-console
REACT_APP_FIREBASE_AUTH_DOMAIN=groundctrl-staging.firebaseapp.com
REACT_APP_FIREBASE_STORAGE_BUCKET=groundctrl-staging.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id

# Development Mode
NODE_ENV=development
